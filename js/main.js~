
function openPage(page) {
	
	var url = "";
	
	switch(page) {
		case "user":
			//url = "/user"
			User.init();
			User.getUserPage({});
			break;
		case "aziende":
			Aziende.init();
			Aziende.getAziendePage({});
			break;
		case "fattura":
			// TEMPORANEO
			$("div#mainContainer").html('<br/><br/><div class="inevidence">Clicca sul pulsante sottostante per scaricare un xls di prova</div><br /><br /><a class="button" href="/download" target="_blank">Scarica</a>');
			break;
		default:
			alert("Unknown");
			return;
			break;
	}
	
	/*$.ajax({
		url:"/user",
		type: "GET",
		data: {},
