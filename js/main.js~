		}
	});*/
	
}

$(function(){
	$('#login').hover(function(){
		$(this).children("img").prop("src", "imgs/google_icon&16_white.png");
	}, function(){
		$(this).children("img").prop("src", "imgs/google_icon&16_black.png");
	});
});